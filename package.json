{
  "name": "repsyps",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "electron-webpack dev",
    "build": "node-gyp configure build --libsdir=$(pwd)/lib",
    "install": "node-gyp configure rebuild --libsdir=$(pwd)/lib",
    "test-native": "node src/native/test.js",
    "clean": "node-gyp clean",
    "build-electron": "electron-webpack",
    "dist": "yarn build-electron && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "build": {
    "appId": "repsyps",
    "asar": false,
    "files": [
      "!src/*",
      "!docs/*",
      "build/*/*",
      "lib/spleeter/**/*",
      "lib/portaudio/lib/**/*",
      "lib/liquid-dsp/libliquid*",
      "lib/libtensorflow/lib/*",
      "lib/ffmpeg-4.2.2/libavutil/libavutil*",
      "lib/ffmpeg-4.2.2/libavcodec/libavcodec*",
      "lib/ffmpeg-4.2.2/libavformat/libavformat*",
      "lib/ffmpeg-4.2.2/libswresample/libswresample*"
    ],
    "directories": {
      "buildResources": "conf/build"
    },
    "mac": {
      "fileAssociations": [
        {
          "ext": "syp",
          "icon": "conf/build/icon.icns",
          "role": "Viewer"
        },
        {
          "ext": "rbind",
          "icon": "conf/build/rbind.icns",
          "role": "Viewer"
        }
      ]
    },
    "win": {
      "fileAssociations": [
        {
          "ext": "syp",
          "description": "Repsyps Project",
          "icon": "conf/build/icon.ico"
        },
        {
          "ext": "rbind",
          "description": "Repsyps Bindings",
          "icon": "conf/build/rbind.ico"
        }
      ]
    },
    "fileAssociations": [
      {
        "ext": "syp"
      },
      {
        "ext": "rbind"
      },
      {
        "ext": "aac"
      },
      {
        "ext": "ac3"
      },
      {
        "ext": "aiff"
      },
      {
        "ext": "caf"
      },
      {
        "ext": "aiff"
      },
      {
        "ext": "flac"
      },
      {
        "ext": "mp4"
      },
      {
        "ext": "m4a"
      },
      {
        "ext": "mp2"
      },
      {
        "ext": "mp3"
      },
      {
        "ext": "oga"
      },
      {
        "ext": "wav"
      }
    ]
  },
  "gypfile": true,
  "electronWebpack": {
    "title": "repsyps",
    "whiteListedModules": [
      "ctyled",
      "redux",
      "react-redux"
    ],
    "renderer": {
      "sourceDirectory": "src/render",
      "webpackConfig": "conf/webpack.renderer.additions.js"
    },
    "main": {
      "sourceDirectory": "src",
      "webpackConfig": "conf/webpack.main.additions.js"
    }
  },
  "dependencies": {
    "@google-cloud/speech": "^3.2.2",
    "@trodi/electron-splashscreen": "^1.0.0",
    "@types/lodash": "^4.14.137",
    "@types/react-redux": "^7.1.7",
    "@types/svgo": "^1.2.0",
    "array-move": "^2.2.1",
    "bezier-easing": "^2.1.0",
    "bindings": "^1.5.0",
    "clone": "^2.1.2",
    "copy-webpack-plugin": "^5.1.1",
    "ctyled": "file:.yalc/ctyled",
    "deox": "^3.2.1",
    "electron-context-menu": "^0.14.0",
    "electron-devtools-installer": "^2.2.4",
    "express": "^4.17.1",
    "file-loader": "^4.2.0",
    "jsondiffpatch": "^0.4.1",
    "lodash": "^4.17.15",
    "node-addon-api": "^2.0.0",
    "node-gyp": "^6.1.0",
    "pad": "^3.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.0",
    "react-sortable-hoc": "^1.11.0",
    "react-svg-loader": "^3.0.3",
    "reduce-reducers": "^1.0.4",
    "redux": "^4.0.5",
    "redux-batched-actions": "^0.4.1",
    "redux-devtools-extension": "^2.13.8",
    "reselect": "^4.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^6.5.4",
    "socket.io": "^2.2.0",
    "source-map-support": "^0.5.12",
    "svgo": "^1.2.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "devtron": "^1.4.0",
    "electron": "5.0.4",
    "electron-builder": "^20.44.4",
    "electron-webpack": "^2.7.1",
    "electron-webpack-ts": "^3.2.0",
    "node-wav": "^0.0.2",
    "typescript": "^3.5.2",
    "webpack": "~4.35.0"
  }
}
