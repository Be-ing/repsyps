{
  "name": "repsyps",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "electron-webpack dev",
    "build": "node-gyp configure build --libsdir=$HOME",
    "test-native": "node src/native/test.js",
    "clean": "node-gyp clean",
    "install": "node-gyp rebuild --libsdir=$HOME",
    "build-electron": "electron-webpack",
    "dist": "yarn build-electron && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "build": {
    "appId": "repsyps",
    "asar": false,
    "files": [
      "!src/*",
      "!docs/*",
      "build/*/*"
    ],
    "directories": {
      "buildResources": "conf/build"
    }
  },
  "gypfile": true,
  "electronWebpack": {
    "title": "repsyps",
    "whiteListedModules": [
      "ctyled",
      "redux-react-hook"
    ],
    "renderer": {
      "sourceDirectory": "src/render",
      "webpackConfig": "conf/webpack.renderer.additions.js"
    },
    "main": {
      "webpackConfig": "conf/webpack.main.additions.js"
    }
  },
  "dependencies": {
    "@google-cloud/speech": "^3.2.2",
    "@types/lodash": "^4.14.137",
    "@types/svgo": "^1.2.0",
    "bindings": "^1.5.0",
    "ctyled": "^0.0.19",
    "deox": "^3.2.1",
    "dsp.js": "^1.0.1",
    "electron-context-menu": "^0.14.0",
    "electron-devtools-installer": "^2.2.4",
    "express": "^4.17.1",
    "file-loader": "^4.2.0",
    "lodash": "^4.17.15",
    "node-addon-api": "^2.0.0",
    "node-gyp": "^6.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-svg-loader": "^3.0.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-react-hook": "^4.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "socket.io": "^2.2.0",
    "source-map-support": "^0.5.12",
    "svgo": "^1.2.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "devtron": "^1.4.0",
    "electron": "5.0.4",
    "electron-builder": "^20.44.4",
    "electron-webpack": "^2.7.1",
    "electron-webpack-ts": "^3.2.0",
    "node-wav": "^0.0.2",
    "typescript": "^3.5.2",
    "webpack": "~4.35.0"
  }
}
